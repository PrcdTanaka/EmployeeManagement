<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>

	<!-- ================================================ Form Bean Definitions -->

	<form-beans>

		<!-- ActionFormã®å®ç¾© -->

		<!-- å¤æ ã¡ã¤ã³ç»é¢ -->
		<form-bean name="MainForm" type="sample.pr.main.MainForm">
			<form-property name="userData" type="java.util.LinkedHashMap" />
		</form-bean>
		<form-bean name="LoginForm" type="sample.pr.main.LoginForm" />
		<form-bean name="SearchForm" type="sample.pr.main.SearchForm" />
		<form-bean name="PasswordForm" type="sample.pr.main.PasswordForm" />
		<form-bean name="RegisterForm" type="sample.pr.main.RegisterForm" />
		<form-bean name="Personal_informationForm" type="sample.pr.main.Personal_informationForm" />
		<form-bean name="Open_RefelenceForm" type="sample.pr.main.Open_RefelenceForm" />
		<form-bean name="Open_informationForm" type="sample.pr.main.Open_informationForm" />
		<form-bean name="PasswordChangeForm" type="sample.pr.main.PasswordChangeForm" />
		<form-bean name="AttendanceForm" type="sample.pr.main.AttendanceForm" />
		<form-bean name="KintaiMainForm" type="sample.pr.main.KintaiMainForm" />
		<form-bean name="KintaiMailForm" type="b02.attendance.inform.KintaiMailForm" />
		<form-bean name="KintaiListForm" type="b01.attendance.main.KintaiListForm" />
		<form-bean name="MonthlyReportForm" type="b03.attendance.monthlyreport.MonthlyReportForm" />
		<form-bean name="ReservationForm" type="a01.room.reservation.ReservationForm" />
		<form-bean name="ReservationConfimForm" type="a02.room.reservation.confirm.ReservationConfirmForm" />
		<form-bean name="Access_Select" type="sample.pr.main.Access_SelectForm" />
		<form-bean name="RoomReservationForm" type="a03.room.register.RoomReservationForm" />
		<form-bean name="KinmuRecordForm" type="sample.pr.main.KinmuRecordForm" />
		<form-bean name="KinmuRecordSendForm" type="sample.pr.main.KinmuRecordSendForm" />
		<form-bean name="KintaiNotificationForm" type="sample.pr.main.KintaiNotificationForm" />
		<form-bean name="AccessSelectForm" type="d01.access.control.AccessSelectForm" />
		<form-bean name="AccessLogSelectForm" type="d02.access.log.AccessLogSelectForm" />
	</form-beans>



	<!-- =========================================== Action Mapping Definitions -->

	<action-mappings>

		<!-- Actionã®å®ç¾© -->

		<!-- å¤æ ã¡ã¤ã³ç»é¢ -->
		<action name="LoginForm" path="/LoginAction" type="sample.pr.main.LoginAction"
			validate="false" scope="request">
			<forward name="login" path="/jsp/login.jsp" />
			<forward name="main" path="/jsp/Main.jsp" />
			<forward name="password" path="/jsp/PasswordForget.jsp" />
		</action>

		<action name="MainForm" path="/MainAction" type="sample.pr.main.MainAction"
			validate="false" scope="request">
			<forward name="logout" path="/jsp/logout.jsp" />
			<forward name="edit" path="/jsp/Personal_information.jsp" />
			<forward name="register" path="/jsp/register.jsp" />
			<forward name="search" path="/jsp/search.jsp" />
			<forward name="password" path="/jsp/Password.jsp" />
			<forward name="attendance" path="/jsp/Attendance.jsp" />
			<forward name="KintaiList" path="/main/B01AttendanceMain/KintaiList.jsp" />
			<forward name="reservation" path="/main/A01Reservation/Reservation.jsp" />
			<forward name="Access" path="/jsp/Access_Control.jsp" />
			<forward name="kinmurecord" path="/jsp/KinmuRecord.jsp"/>
			<forward name="kintaiNotification" path="/jsp/KintaiNotification.jsp"/>
			<forward name="accessControl" path="/WEB-INF/jsp/D01AccessControl/AccessControlMain.jsp"/>
		</action>

		<action name="PasswordForm" path="/PasswordAction"
			type="sample.pr.main.PasswordAction" validate="false" scope="request">
			<forward name="main" path="/jsp/Main.jsp" />
			<forward name="password" path="/jsp/Password.jsp" />
		</action>

		<action name="RegisterForm" path="/RegisterAction"
			type="sample.pr.main.RegisterAction" validate="false" scope="request">
			<forward name="register" path="/jsp/register.jsp" />
			<forward name="main" path="/jsp/Main.jsp" />
		</action>

		<action name="SearchForm" path="/SearchAction" type="sample.pr.main.SearchAction"
			validate="false" scope="request">
			<forward name="search" path="/jsp/search.jsp" />
			<forward name="back" path="/jsp/Main.jsp" />
			<forward name="fly" path="/jsp/Open_RefeRegistration.jsp" />
		</action>


		<action name="Personal_informationForm" path="/Personal_informationAction"
			type="sample.pr.main.Personal_informationAction" validate="false"
			scope="request">
			<forward name="main" path="/jsp/Main.jsp" />
			<forward name="pInfo" path="/jsp/Personal_information.jsp" />
			<forward name="search" path="/jsp/search.jsp" />
		</action>

		<action name="Open_informationForm" path="/Open_informationAction"
			type="sample.pr.main.Open_informationAction" validate="false" scope="request">
			<forward name="main" path="/jsp/Main.jsp" />
			<forward name="open" path="/jsp/open_inforegistration.jsp" />
		</action>

		<action name="Open_RefelenceForm" path="/Open_RefelenceAction"
			type="sample.pr.main.Open_RefelenceAction" validate="false" scope="request">
			<forward name="main" path="/jsp/Main.jsp" />
			<forward name="search" path="/jsp/search.jsp" />
		</action>

		<action name="AttendanceForm" path="/AttendanceAction"
			type="sample.pr.main.AttendanceAction" validate="false" scope="request">
			<forward name="main" path="/jsp/Main.jsp" />
			<forward name="attendance" path="/jsp/Attendance.jsp" />
		</action>

		<action name="PasswordForm" path="/PasswordForgetAction"
			type="sample.pr.main.PasswordForgetAction" validate="false" scope="request">
			<forward name="change" path="/jsp/PasswordChange.jsp" />
			<forward name="login" path="/jsp/login.jsp" />
			<forward name="Forget" path="/jsp/PasswordForget.jsp" />
		</action>

		<action name="PasswordForm" path="/PasswordChangeAction"
			type="sample.pr.main.PasswordChangeAction" validate="false" scope="request">
			<forward name="login" path="/jsp/login.jsp" />
			<forward name="change" path="/jsp/PasswordChange.jsp" />
		</action>

		<action name="KintaiMainForm" path="/KintaiMainAction"
			type="sample.pr.main.KintaiMainAction" validate="false" scope="request">
			<forward name="main" path="/jsp/Main.jsp" />
			<forward name="kintaimail" path="/jsp/KintaiMail.jsp" />
			<forward name="kintailist" path="/jsp/KintaiList.jsp" />
		</action>
		<action name="KintaiMailForm" path="/KintaiMailAction"
			type="b02.attendance.inform.KintaiMailAction" validate="false" scope="request">
			<forward name="KintaiMain" path="/jsp/KintaiMain.jsp" />
			<forward name="kintaimail" path="/WEB-INF/jsp/B02AttendanceInform/KintaiMail.jsp" />
			<forward name="kintaieditor" path="/WEB-INF/jsp/B02AttendanceInform/KintaiEditor.jsp" />
			<forward name="kintailist" path="/main/B01AttendanceMain/KintaiList.jsp" />
			<forward name="kintaimailcomp" path="/WEB-INF/jsp/B02AttendanceInform/KintaiMailComp.jsp" />
			<forward name="main" path="/jsp/Main.jsp" />
		</action>
		<action name="KintaiListForm" path="/KintaiListAction"
			type="b01.attendance.main.KintaiListAction" validate="false" scope="request">
			<forward name="kintaimail" path="/WEB-INF/jsp/B02AttendanceInform/KintaiMail.jsp" />
			<forward name="MonthlyReport" path="/WEB-INF/jsp/B03AttendanceMonthlyreport/MonthlyReport.jsp" />
			<forward name="kintaieditor" path="/WEB-INF/jsp/B02AttendanceInform/KintaiEditor.jsp" />
			<forward name="main" path="/jsp/Main.jsp" />
		</action>
		<action name="MonthlyReportForm" path="/MonthlyReportAction"
			type="b03.attendance.monthlyreport.MonthlyReportAction" validate="false" scope="request">
			<forward name="MonthlyReport" path="/WEB-INF/jsp/B03AttendanceMonthlyreport/MonthlyReport.jsp" />
			<forward name="kintailist" path="/main/B01AttendanceMain/KintaiList.jsp" />
			<forward name="kintaieditor" path="/WEB-INF/jsp/B02AttendanceInform/KintaiEditor.jsp" />
			<forward name="MonthlyReportcomp" path="/WEB-INF/jsp/B03AttendanceMonthlyreport/MonthlyReportComp.jsp" />
		</action>
		<action name="ReservationForm" path="/ReservationAction"
			type="a01.room.reservation.ReservationAction" validate="false" scope="request">
			<forward name="reservation" path="/main/A01Reservation/Reservation.jsp" />
			<forward name="confirm" path="/WEB-INF/jsp/A02ReservationConfirm/ReservationConfirm.jsp" />
			<forward name="room" path="/WEB-INF/jsp/A03RoomReservation/RoomReservation.jsp" />
			<forward name="main" path="/jsp/Main.jsp" />
		</action>
		<action name="KinmuRecordForm" path="/KinmuRecordAction"
			type="sample.pr.main.KinmuRecordAction" validate="false" scope="request">
			<forward name="main" path="/jsp/Main.jsp" />
			<forward name="kinmurecord" path="/jsp/KinmuRecord.jsp" />
		</action>

		<action name="RoomReservationForm" path="/RoomReservationAction"
			type="a03.room.register.RoomReservationAction" validate="false" scope="request">
			<forward name="room" path="/WEB-INF/jsp/A03RoomReservation/RoomReservation.jsp" />
			<forward name="reservation" path="/main/A01Reservation/Reservation.jsp" />
		</action>
		<action name="ReservationConfirmForm" path="/ReservationConfirmAction"
			type="a02.room.reservation.confirm.ReservationConfirmAction" validate="false" scope="request">
			<forward name="main" path="/WEB-INF/jsp/A01Reservation/Reservation.jsp" />
			<forward name="confirm" path="/WEB-INF/jsp/A02ReservationConfirm/ReservationConfirm.jsp" />
		</action>
		<action name="KintaiNotificationForm" path="/KintaiNotificationAction"
				type="sample.pr.main.KintaiNotificationAction" validate="false" scope="request">
			<forward name="main" path="/jsp/Main.jsp" />
			<forward name="kintaiNotification" path="/jsp/KintaiNotification.jsp"/>
		</action>

		<action name="AccessSelectForm" path="/AccessSelectAction"
				type="d01.access.control.AccessSelectAction" validate="false" scope="request">
			<forward name="entry" path="/WEB-INF/jsp/D01AccessControl/AccessControlEntry.jsp" />
			<forward name="exit" path="/WEB-INF/jsp/D01AccessControl/AccessControlExit.jsp" />
			<forward name="reEntry" path="/WEB-INF/jsp/D01AccessControl/AccessControlReEntry.jsp" />
			<forward name="reExit" path="/WEB-INF/jsp/D01AccessControl/AccessControlReExit.jsp" />
		</action>
		<action name="AccessSelectForm" path="/InsertAccess"
				type="d01.access.control.InsertAccess" validate="false" scope="request">
			<forward name="complete" path="/WEB-INF/jsp/D01AccessControl/AccessCompleteEntry.jsp" />
			<forward name="reComplete" path="/WEB-INF/jsp/D01AccessControl/AccessCompleteReEntry.jsp" />
		</action>
		<action name="AccessSelectForm" path="/UpdateAccess"
				type="d01.access.control.UpdateAccess" validate="false" scope="request">
			<forward name="complete" path="/WEB-INF/jsp/D01AccessControl/AccessCompleteExit.jsp" />
			<forward name="reComplete" path="/WEB-INF/jsp/D01AccessControl/AccessCompleteReExit.jsp" />
		</action>
		<action name="AccessLogSelectForm" path="/AccessLogSelectAction"
				type="d02.access.log.AccessLogSelectAction" validate="false" scope="request">
			<forward name="AccessLogSelect" path="/WEB-INF/jsp/D02AccessLog/AccessLogTable.jsp" />
		</action>



	</action-mappings>


	<!-- ======================================== Message Resources Definitions -->

	<message-resources parameter="MessageResources" />
</struts-config>

